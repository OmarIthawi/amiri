      uni0627'      lookup LamAlefIsol;

  sub uni0644.fina' lookup LamAlefFina
      uni0621'      lookup FloatHamza
      uni0627'      lookup LamAlefFina;

  # everything else
  sub @before'      lookup Before
      @floats.isol' lookup MedialFloats
      @after'       lookup After;

